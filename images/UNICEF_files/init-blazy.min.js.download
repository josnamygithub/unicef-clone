jQuery(document).ready(function(){new Blazy({breakpoints:[{width:480,src:"data-src-mouse"},{width:768,src:"data-src-dog"}],success:function(e){setTimeout(function(){e.parentElement.classList.remove("blazy-container")},200)},error:function(e,t){}})});const images=document.querySelectorAll("img.b-lazy"),observer=new IntersectionObserver((e=>{e.forEach((e=>{e.isIntersecting&&e.target.classList.contains("b-lazy")&&e.target.parentElement.classList.remove("blazy-container")}))}));images.forEach((e=>{observer.observe(e)}));